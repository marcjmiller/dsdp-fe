{
	"name": "frontend",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@emotion/react": "^11.9.0",
		"@emotion/styled": "^11.8.1",
		"@mui/icons-material": "^5.8.2",
		"@mui/material": "^5.8.2",
		"@types/node": "^12.0.0",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"axios": "^0.26.0",
		"js-file-download": "^0.4.12",
		"pretty-bytes": "^5.6.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-dropzone": "^14.2.1",
		"react-scripts": "4.0.3",
		"typescript": "^4.1.2 <4.5.0",
		"web-vitals": "^1.0.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"prepare": "husky install",
		"build-backend": "cd ../backend && earthly +build-dev-image",
		"server": "earthly +build-dev && docker compose up",
		"lint": "eslint {src,cypress}/**/*.ts*",
		"lint:fix": "eslint --fix {src,cypress}/**/*.ts*",
		"format": "prettier --write './{src,cypress}/**/*.{ts,tsx,css,json}'",
		"cypress": "cypress open --config-file cypress/config/development.json",
		"test": "react-scripts test --watchAll",
		"test:unit": "react-scripts test --coverage --watchAll",
		"test:e2e": "start-server-and-test server http://localhost:3000 test:e2e-dev",
		"test:e2e-ci": "npx cypress run --browser chrome --config-file cypress/config/pipeline.json",
		"test:e2e-dev": "npx cypress run --browser firefox --config-file cypress/config/development.json"
	},
	"proxy": "http://api:8000",
	"jest": {
		"coverageThreshold": {
			"global": {
				"branches": 80,
				"functions": 80,
				"lines": 80,
				"statements": -10
			}
		},
		"coveragePathIgnorePatterns": [
			"utils.ts",
			"reportWebVitals.ts",
			"theme.ts",
			"src/tests/*",
			"src/index.tsx"
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/dom": "^8.11.1",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^26.0.15",
		"@typescript-eslint/eslint-plugin": "^4.0.0",
		"@typescript-eslint/parser": "^4.0.0",
		"babel-eslint": "^10.0.0",
		"cypress": "^9.1.0",
		"cypress-file-upload": "^5.0.8",
		"eslint": "^7.12.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-react-app": "^6.0.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.27.0",
		"eslint-plugin-react-hooks": "^4.0.8",
		"husky": "^7.0.0",
		"jsonwebtoken": "^8.5.1",
		"msw": "^0.42.0",
		"prettier": "^2.4.1",
		"start-server-and-test": "^1.14.0"
	},
	"resolutions": {
		"ejs": "3.1.8",
		"nth-check": "2.0.1"
	}
}
